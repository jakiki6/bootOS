org 0x7c00

%include "../sysmap.inc"

main:
        mov al, "@"
        call bootOS.input_line
        call bootOS.xdigit             ; Get a hexadecimal digit
        mov cl,4
        shl al,cl
        xchg ax,cx
        call bootOS.xdigit             ; Get a hexadecimal digit
        or al,cl
        mov byte [bootOS._disk2 + 2], al
	int 0x20
