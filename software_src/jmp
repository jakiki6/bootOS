org 0x7c00

jmp_command:
        mov al, ":"
        call input_line
	mov si, 0x7780
	xor ax, ax
	call convert
	cs mov byte [0x7a00 + 0d294], al
	xor ax, ax
	call convert
	cs mov byte [0x7a00 + 0d295], al
	xor ax, ax
	call convert
	cs mov byte [0x7a00 + 0d292], al
	int 0x20

convert:
        call xdigit             ; Get a hexadecimal digit
        mov cl,4
        shl al,cl
        xchg ax,cx
        call xdigit             ; Get a hexadecimal digit
        or al,cl
        ret
xdigit:  
        lodsb
        cmp al,0x00             ; Zero character marks end of line
        je r
        sub al,0x30             ; Avoid spaces (anything below ASCII 0x30)
        jc xdigit
        cmp al,0x0a
        jc r
        sub al,0x07
        and al,0x0f   
        stc
r:
        ret
input_line:   
	push ax
        int 0x26
	pop ax
	ret
