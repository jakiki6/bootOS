org 0x7c00

%include "../sysmap.inc"

jmp_command:
        mov al, ":"
        call bootOS.input_line
	mov si, 0x7780
	xor ax, ax
	call bootOS.xdigit
	cs mov byte [bootOS._disk2 + 1], al
	xor ax, ax
	call bootOS.xdigit
	cs mov byte [bootOS._disk2 + 2], al
	xor ax, ax
	call bootOS.xdigit
	cs mov byte [bootOS._disk1 + 1], al
	int 0x20
